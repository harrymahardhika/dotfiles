{
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter",
    "editor.formatOnSave": false
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.formatOnSave": true,
    "editor.insertSpaces": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[vue]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "alt-catppuccin.boldKeywords": false,
  "alt-catppuccin.italicComments": false,
  "alt-catppuccin.italicKeywords": false,
  "alt-catppuccin.syntaxMode": "default",
  "alt-catppuccin.workbenchMode": "flat",
  "apc.electron": {
    "titleBarStyle": "hidden"
  },
  "apc.font.family": "JetBrainsMono Nerd Font",
  "apc.monospace.font.family": "JetBrainsMono Nerd Font, Geist Mono",
  "apc.stylesheet": {
    ".decorationsOverviewRuler": "display: none; width: 0 !important;",
    ".editor-group-container .title": "pointer-events: none;",
    ".editor-group-container .title *": "display: none;",
    ".editor-group-container.empty": "-webkit-app-region: drag;",
    ".letterpress": "display:none !important",
    ".monaco-editor .scroll-decoration": "box-shadow: none;",
    ".quick-input-widget": "top: 25% !important; font-size: 14px;",
    ".quick-input-widget .monaco-inputbox": "padding: 8px; !important; font-size: 15px !important;"
  },
  "bladeFormatter.format.indentSize": 2,
  "bladeFormatter.format.noMultipleEmptyLines": true,
  "bladeFormatter.format.sortHtmlAttributes": "code-guide",
  "bladeFormatter.misc.dontShowNewVersionMessage": true,
  "breadcrumbs.enabled": false,
  "window.zoomLevel": 0,
  "breadcrumbs.filePath": "off",
  "cSpell.userWords": ["Doesnt", "Laravel", "stocklogs"],
  "catppuccin.italicComments": false,
  "catppuccin.italicKeywords": false,
  "code-runner.clearPreviousOutput": true,
  "code-runner.enableAppInsights": false,
  "codesnap.showLineNumbers": false,
  "codesnap.showWindowControls": false,
  "codesnap.shutterAction": "copy",
  "codesnap.transparentBackground": true,
  "debug.disassemblyView.showSourceCode": false,
  "debug.javascript.codelens.npmScripts": "never",
  "diffEditor.codeLens": true,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeLensFontFamily": "JetBrainsMono Nerd Font, Geist Mono",
  "editor.cursorWidth": 1,
  "editor.detectIndentation": true,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "JetBrainsMono Nerd Font, Geist Mono, Hack, SF Mono, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13.5,
  "editor.fontWeight": 500,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.indentation": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlayHints.fontFamily": "JetBrainsMono Nerd Font, Geist Mono",
  "editor.inlayHints.fontSize": 12,
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": false,
  "editor.letterSpacing": 0,
  "editor.lineHeight": 1.4,
  "editor.lineNumbers": "off",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "never",
  "editor.minimap.autohide": "mouseover",
  "editor.minimap.maxColumn": 90,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 1,
  "editor.overviewRulerBorder": false,
  "editor.quickSuggestionsDelay": 300,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.rulers": [],
  "editor.scrollBeyondLastLine": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.semanticHighlighting.enabled": true,
  "editor.showFoldingControls": "never",
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.tabSize": 2,
  "editor.useTabStops": true,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "none",
  "editorconfig.generateAuto": false,
  "emmet.includeLanguages": {
    "blade": "html"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": true,
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.visible": 1,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.css": "tailwindcss",
    "*.eex": "HTML (EEx)"
  },
  "files.autoSave": "onFocusChange",
  "files.hotExit": "off",
  "files.insertFinalNewline": true,
  "files.restoreUndoStack": false,
  "files.saveConflictResolution": "overwriteFileOnDisk",
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.detectSubmodulesLimit": 5,
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "css": true,
    "markdown": true,
    "plaintext": true,
    "scminput": true
  },
  "github.gitProtocol": "ssh",
  "gitlens.telemetry.enabled": false,
  "glassit.alpha": 250,
  "go.toolsManagement.autoUpdate": true,
  "html.format.indentHandlebars": true,
  "intelephense.completion.fullyQualifyGlobalConstantsAndFunctions": true,
  "intelephense.completion.maxItems": 20,
  "intelephense.environment.phpVersion": "8.3",
  "intelephense.telemetry.enabled": false,
  "javascript.format.semicolons": "remove",
  "javascript.preferences.quoteStyle": "single",
  "javascript.referencesCodeLens.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.validate.enable": true,
  "jupyter.logging.level": "error",
  "keyboard.dispatch": "keyCode",
  "laravel-pint.enable": true,
  "markdown-pdf.displayHeaderFooter": false,
  "markdown-pdf.emoji": false,
  "markdown-pdf.outputDirectoryRelativePathFile": true,
  "markdown-pdf.printBackground": false,
  "markdown-pdf.styles": ["/home/harry/Tools/markdown-pdf/markdown-pdf.css"],
  "markdown.marp.chromePath": "/usr/bin/google-chrome",
  "material-icon-theme.activeIconPack": "none",
  "material-icon-theme.files.color": "#42a5f5",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.saturation": 1,
  "oneDarkPro.editorTheme": "Nord",
  "oneDarkPro.italic": false,
  "prettier.printWidth": 100,
  "prettier.requireConfig": true,
  "prettier.semi": false,
  "prettier.singleQuote": false,
  "prettier.trailingComma": "es5",
  "scm.diffDecorations": "gutter",
  "scm.diffDecorationsGutterPattern": {
    "modified": false
  },
  "scm.diffDecorationsGutterWidth": 1,
  "tailwindCSS.colorDecorators": false,
  "tailwindCSS.emmetCompletions": true,
  "terminal.integrated.fontSize": 13.5,
  "terminal.integrated.fontWeight": "500",
  "terminal.integrated.lineHeight": 1.2,
  "typescript.format.semicolons": "remove",
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "single",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "manual",
  "vue.autoInsert.dotValue": true,
  "vue.server.hybridMode": true,
  "window.autoDetectHighContrast": false,
  "window.commandCenter": false,
  "window.customMenuBarAltFocus": false,
  "window.density.editorTabHeight": "compact",
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "toggle",
  "window.title": "${activeEditorShort}${separator}${folderName}",
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.decorations.colors": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.excludeDirty": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "workbench.editor.restoreViewState": false,
  "workbench.editor.showTabs": "none",
  "workbench.editorAssociations": {
    "*.sqlite": "default"
  },
  "workbench.layoutControl.enabled": false,
  "workbench.layoutControl.type": "menu",
  "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.renderIndentGuides": "none",
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "zenMode.showTabs": "none",
  "editor.inlayHints.enabled": "off",
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "apc.theme": [],
  "workbench.statusBar.visible": false
}
